{
  "private": true,
  "name": "google-globe-trends",
  "version": "1.0.0",
  "description": "Create beautiful and interactive Google Trends globe visualizations with ease.",
  "license": "MIT",
  "homepage": "https://globe-trends.netlify.com/",
  "repository": "GArdennes/google-globe-trends",
  "bugs": "https://github.com/GArdennes/google-globe-trends/issues",
  "author": "Kevin Afachao",
  "keywords": [
    "google",
    "globe",
    "trends",
    "react-globe",
    "react",
    "three",
    "threejs",
    "webgl",
    "visualization",
    "interactive",
    "earth",
    "netlify",
    "jamstack"
  ],
  "scripts": {
    "crawl": "npm run untrack && babel-node ./src/data/crawl.js",
    "build": "npm run clean && npm run crawl && vite build",
    "clean": "rm -rf dist",
    "dev": "vite",
    "lint": "eslint --fix .",
    "preview": "vite preview",
    "start": "vite",
    "untrack": "git update-index --assume-unchanged ./src/data/data.json"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "moment": "^2.30.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-globe.gl": "^2.33.2",
    "sass": "^1.56.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "google-trends-api": "^4.9.0",
    "prettier": "^2.0.5",
    "vite": "^5.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },

  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
